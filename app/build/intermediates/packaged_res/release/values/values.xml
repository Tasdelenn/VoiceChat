    <style name="ActionButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/margin_normal</item>
        <item name="android:minWidth">@dimen/button_min_width</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="Chronometer">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/chronometer_text_size</item>
        <item name="android:layout_marginBottom">@dimen/margin_normal</item>
    </style>
    <style name="StatusText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/margin_large</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/status_text_size</item>
    </style>
    <style name="Theme.VoiceChat" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryVariant">@color/primary_dark</item>
        <item name="colorOnPrimary">@color/white</item>
        
        
        <item name="colorSecondary">@color/accent</item>
        <item name="colorSecondaryVariant">@color/accent</item>
        <item name="colorOnSecondary">@color/black</item>
        
        
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        
        
        <item name="android:textColorPrimary">@color/text_primary</item>
        <item name="android:textColorSecondary">@color/text_secondary</item>
        
        
        <item name="materialButtonStyle">@style/Widget.VoiceChat.Button</item>
    </style>
    <style name="Widget.VoiceChat.Button" parent="Widget.MaterialComponents.Button">
        <item name="android:textColor">@color/white</item>
        <item name="backgroundTint">@color/button_enabled</item>
        <item name="android:minHeight">56dp</item>
        <item name="android:padding">@dimen/margin_normal</item>
        <item name="cornerRadius">8dp</item>
    </style>
    <style name="Widget.VoiceChat.Button.Playing">
        <item name="backgroundTint">@color/status_playing</item>
    </style>
    <style name="Widget.VoiceChat.Button.Recording">
        <item name="backgroundTint">@color/status_recording</item>
    </style>
</resources>